<Page x:Class="Infonet.CStoreCommander.UI.View.Sale.NumberpadForBottleItems"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:Infonet.CStoreCommander.UI.View.Sale"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:controls="using:Infonet.CStoreCommander.UI.Controls"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">
    
    <controls:NumberPad IsAdditionEnabled="False"
                        x:Uid="txtSetQuantity"
                        BackNavigation="{x:Bind BottleReturnsScreenVM.SecondFrameBackNavigationCommand, Mode=OneWay}"
                        IsSubtractionEnabled="True"
                        EnterTappedCommand="{x:Bind BottleReturnsScreenVM.UpdateQuantityCommand}" />

</Page>
