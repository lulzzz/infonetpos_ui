<Page x:Class="Infonet.CStoreCommander.UI.View.PumpOptions.Fuel_Pricing.FuelPriceQuantityPad"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:Infonet.CStoreCommander.UI.View.PumpOptions.Fuel_Pricing"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:controls="using:Infonet.CStoreCommander.UI.Controls"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">

    <controls:NumberPad IsAdditionEnabled="False"
                        IsSubtractionEnabled="False"
                        BackArrowVisibility="Collapsed"
                        x:Uid="Manual"
                        IsDollarVisible="Visible"
                        CashButtonItemSource="{x:Bind AddManualVM.CashButtons, Mode =OneWay}"
                        EnterTappedCommand="{x:Bind AddManualVM.SetAmountCommand, Mode=OneWay}" />
</Page>
